  "license": "MIT",
  "scripts": {
    "ng": "nx",
    "start": "NX_DAEMON=false nx serve",
    "build:local": "NX_DAEMON=false npx nx build bv-admin -c local --verbose --output-hashing=all --base-href /bv-admin/ --skip-nx-cache",
    "build:heikeqs": "NX_DAEMON=false npx nx build bv-admin -c heikeqs --verbose --output-hashing=all --base-href /bv-admin/ --skip-nx-cache",
    "build:prod": "NX_DAEMON=false npx nx run bv-admin:build:production --verbose --output-hashing=all --base-href /bv-admin/ --skip-nx-cache",
    "test": "NX_DAEMON=false npx nx test",
    "test:all": "NX_DAEMON=false npx nx run-many --target=test --all",
    "lint:all": "NX_DAEMON=false npx nx run-many --target=lint --all",
    "full-check": "NX_DAEMON=false npx nx run-s lint:all test:all build",
    "sheriff": "NX_DAEMON=false npx sheriff verify src/main.ts"
  },
  "private": true,
